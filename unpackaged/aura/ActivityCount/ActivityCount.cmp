<aura:component implements="force:appHostable,force:hasRecordId,flexipage:availableForAllPageTypes" access="global" controller='AccountDashboardController'>
    <aura:attribute name="recordId" type="Id"/>
    <aura:attribute name="users" type="Map"/>
    <aura:handler name="init" value="{!this}" action="{!c.doinit}"/>
    <lightning:tabset selectedTabId="two">
        <lightning:tab label="Account Actvities" id="one">
            {!v.users.AccountActivity}
        </lightning:tab>
        <lightning:tab label="Opportunity Activities" id="two">
           {!v.users.OppActivity}
        </lightning:tab>
        <lightning:tab label="Total Activities" id="three">
            {!v.users.total}
        </lightning:tab>
    </lightning:tabset>
</aura:component>